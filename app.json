{
  "expo": {
    "name": "AccessLink LGBTQ+",
    "slug": "accesslink-lgbtq",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#6c5ce7"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.accesslinklgbtq.lgbtq",
      "infoPlist": {
        "NSCalendarsUsageDescription": "AccessLink adds events you choose to your calendar so you never miss community happenings.",
        "NSRemindersUsageDescription": "AccessLink can optionally add reminders for events you save."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#6c5ce7"
      },
      "package": "com.accesslinklgbtq.lgbtq",
      "versionCode": 1,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
  "android.permission.CALL_PHONE",
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR"
      ],
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "buildToolsVersion": "34.0.0"
    },
    "web": {
      "bundler": "metro",
      "template": "./web/index.html",
      "config": {
        "firebase": {
          "measurementId": "G-XXXXXXXXXX"
        }
      }
    },
    "scheme": "accesslink",
    "owner": "chriseckman",
    "updates": {
      "url": "https://u.expo.dev/chriseckman/accesslink-lgbtq"
    },
    "sdkVersion": "51.0.0",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow AccessLink to use your location to find nearby LGBTQ+ friendly businesses."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "351daa17-f3ee-470c-9936-54eb17100775"
      }
    }
  }
}

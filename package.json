{
  "name": "accesslink-lgbtq",
  "version": "1.0.0",
  "description": "Accessible LGBTQ+ business directory and community platform",
  "homepage": "https://accesslinklgbtq.app/",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "FIREBASE_INTEGRATION_TESTS=true jest --testPathPattern=integration",
    "test:firebase": "npm run firebase:emulators & sleep 3 && npm run test:integration && npm run firebase:stop",
    "firebase:emulators": "firebase emulators:start --only auth,firestore --project demo-project",
    "firebase:stop": "firebase emulators:stop",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/cli-server-api": "^18.0.0",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/stack": "^6.3.0",
    "dotenv": "^17.2.1",
    "expo": "~51.0.0",
    "expo-auth-session": "~5.5.2",
    "expo-constants": "~16.0.0",
    "expo-crypto": "~13.0.2",
    "expo-dev-client": "~4.0.20",
    "expo-font": "~12.0.0",
    "expo-location": "~17.0.0",
    "expo-sqlite": "~14.0.6",
    "expo-status-bar": "~1.12.0",
    "expo-web-browser": "~13.0.3",
    "firebase": "^12.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-paper": "^5.12.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.2.79",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "babel-jest": "^30.0.5",
    "eslint": "^8.57.0",
    "firebase-tools": "^14.11.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "react-test-renderer": "^18.3.1",
    "typescript": "~5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/slackerchris/AccessLink-LGBTQ.git"
  },
